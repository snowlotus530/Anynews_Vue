{
  "name": "anynews",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run generate_icons && npm run generate_splash && vue-cli-service build",
    "generate_icons": "npx vue-pwa-asset-generator -a ./src/assets/icons/logo.svg -o ./public/img/icons && cp ./public/img/icons/favicon.ico ./public/",
    "generate_splash": "./node_modules/pwa-asset-generator/bin/cli ./src/assets/icons/logo.svg --padding '0 max(20vw,calc(50vw - 150px))' ./public/img/splash",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "@seregpie/vue-resize-sensor": "^1.0.0",
    "@vue/composition-api": "^1.0.0-rc.2",
    "core-js": "^3.6.5",
    "dexie": "^3.0.3",
    "hls.js": "^0.13.2",
    "jszip": "^3.5.0",
    "pdfmake": "^0.1.68",
    "raw-loader": "^2.0.0",
    "register-service-worker": "^1.7.1",
    "sanitize-html": "^2.4.0",
    "v-viewer": "^1.6.3",
    "velocity-animate": "^1.5.2",
    "vue": "^2.6.11",
    "vue-i18n": "^8.20.0",
    "vue-moment": "^4.1.0",
    "vue-router": "^3.4.2",
    "vue2-touch-events": "^2.2.1",
    "vuetify": "^2.3.8",
    "vuex": "^3.5.1",
    "vuex-persist": "^2.2.0",
    "webfontloader": "^1.6.28",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.3",
    "@vue/cli-plugin-eslint": "^4.5.3",
    "@vue/cli-plugin-pwa": "^4.5.3",
    "@vue/cli-service": "^4.5.3",
    "axios": "^0.21.1",
    "babel-eslint": "^10.1.0",
    "before-build-webpack": "^0.2.11",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^7.6.0",
    "eslint-plugin-vuetify": "^1.0.0-beta.7",
    "material-design-icons-iconfont": "^6.1.0",
    "pwa-asset-generator": "^4.1.1",
    "stylus": "^0.54.8",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-axios": "0.0.4",
    "vue-cli-plugin-vuetify": "^2.0.7",
    "vue-template-compiler": "^2.6.11",
    "webpack-dev-server": "^3.11.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "vars": "all",
          "args": "after-used",
          "ignoreRestSiblings": false,
          "argsIgnorePattern": "[iI]gnored"
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
